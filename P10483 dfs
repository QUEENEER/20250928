#include <bits/stdc++.h>
#include <cstdlib>
using namespace std;
#define int long long
int n,w;
int ans=n;
int puch[20];
int sum[20]={0};
void dfs(int u,int v){
	if(v>=ans){
		return;
	}
	if(u==n){
		ans=v;
		return;
	}
	for(int i=0;i<v;i++){
		if(sum[i]+puch[u]<=w){
			sum[i]+=puch[u];
			dfs(u+1,v);
			sum[i]-=puch[u];
		}
	}
	sum[v]=puch[u];
	dfs(u+1,v+1);
	sum[v]=0;
	
}
signed main(){
	cin>>n>>w;
	for(int i=0;i<n;i++){
		cin>>puch[i];
	}
	sort(puch,puch+n,greater<int>());
	dfs(0,0);
	cout<<ans;
}
